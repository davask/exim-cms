{% extends '@theme/layout.html.twig' %}

{% block page_content %}
    {% if page is defined %}
        <div class="col-sm-12">
            {% if page.name != 'global'%}
                {{ sonata_page_render_container('content_search', 'global') }}
            {% endif %}
            {{ sonata_page_render_container('content_search', page) }}
        </div>
    {% endif %}
{% endblock %}
