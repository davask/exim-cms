{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Speaker</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Isspeaker</th>
                <td>{{ entity.isSpeaker }}</td>
            </tr>
            <tr>
                <th>Career</th>
                <td>{{ entity.career }}</td>
            </tr>
            <tr>
                <th>Specialties</th>
                <td>{{ entity.specialties }}</td>
            </tr>
            <tr>
                <th>Publications</th>
                <td>{{ entity.publications }}</td>
            </tr>
            <tr>
                <th>Latitude</th>
                <td>{{ entity.latitude }}</td>
            </tr>
            <tr>
                <th>Longitude</th>
                <td>{{ entity.longitude }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('intervenant') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('intervenant_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
